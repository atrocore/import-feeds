{
  "controller": "controllers/record",
  "boolFilterList": [
    "onlyMy",
    "onlyExecutable"
  ],
  "hiddenBoolFilterList": [
    "onlyExecutable"
  ],
  "iconClass": "fas fa-cloud-download-alt",
  "views": {
    "detail": "import:views/import-feed/detail"
