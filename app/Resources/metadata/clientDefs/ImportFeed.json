{
  "controller": "controllers/record",
  "boolFilterList": [
    "onlyMy"
  ],
  "iconClass": "fas fa-cloud-download-alt",
  "recordViews": {
    "detail": "import:views/import-feed/record/detail"
  },
  "relationshipPanels": {
    "importResults": {
      "create": false,
      "select": false,
      "rowActionsView": "views/record/row-actions/view-and-remove",
      "view": "import:views/import-feed/record/panels/import-results"
    }
  },
  "bottomPanels": {
    "detail": [
      {
        "name": "simpleTypeSettings",
        "label": "Simple Type Settings",
        "view": "import:views/import-feed/record/panels/simple-type-settings"
      }
    ]
  },
  "optionalBottomPanels": {
    "simpleTypeSettings": [
      {
        "field": "type",
        "value": "simple"
      }
    ]
  },
  "additionalDropdownItems": {
    "runImport": {
      "name": "runImport",
      "label": "runImport",
      "actionViewPath": "import:views/import-feed/actions/run-import",
      "action": "runImport",
      "optionsToPass": [
        "model"
      ],
      "iconClass": "far fa-arrow-alt-circle-up",
      "conditions": [
        {
          "type": "type",
          "value": "detail"
        }
      ]
    }
  },
  "additionalRowActions": {
    "runImport": {
      "name": "runImport",
      "label": "runImport",
      "actionViewPath": "import:views/import-feed/actions/run-import",
      "action": "runImport",
      "optionsToPass": [
        "model"
      ],
      "iconClass": "far fa-arrow-alt-circle-up"
    }
  },
  "configuratorButtons": {
    "Product": [
      {
        "name": "addAttribute",
        "action": "addAttribute"
      }
    ]
  },
  "customEntities": {
    "productCategories": {
      "entity": "Category"
    }
  },
  "dynamicLogic": {
    "fields": {
      "file": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "isNotEmpty",
              "attribute": "id"
            },
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "type": {
        "readOnly": {
          "conditionGroup": [
            {
              "type": "isNotEmpty",
              "attribute": "id"
            }
          ]
        }
      },
      "isFileHeaderRow": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "fileFieldDelimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "fileTextQualifier": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "decimalMark": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "fileDataAction": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "entity": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "delimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            }
          ]
        }
      },
      "idField": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            },
            {
              "type": "notEquals",
              "attribute": "fileDataAction",
              "value": "create"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "type",
              "value": "simple"
            },
            {
              "type": "notEquals",
              "attribute": "fileDataAction",
              "value": "create"
            }
          ]
        }
      }
    }
  }
}
