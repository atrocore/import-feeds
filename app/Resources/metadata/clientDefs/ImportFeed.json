{
  "controller": "controllers/record",
  "boolFilterList": [
    "onlyMy",
    "onlyImportFailed24Hours",
    "onlyImportFailed7Days",
    "onlyImportFailed28Days"
  ],
  "iconClass": "fas fa-cloud-download-alt",
  "views": {
    "detail": "import:views/import-feed/detail"
  },
  "recordViews": {
    "detail": "import:views/import-feed/record/detail"
  },
  "relationshipPanels": {
    "configuratorItems": {
      "select": false,
      "dragDrop": {
        "isActive": true,
        "maxSize": 20,
        "sortField": "sortOrder"
      },
      "rowActionsView": "views/record/row-actions/edit-and-remove"
    },
    "importJobs": {
      "create": false,
      "select": false,
      "rowActionsView": "import:views/import-job/record/row-actions/relationship",
      "view": "import:views/import-feed/record/panels/import-jobs"
    }
  },
  "dynamicLogic": {
    "fields": {
      "maxPerJob": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "format",
              "value": [
                "CSV",
                "Excel"
              ]
            }
          ]
        }
      },
      "type": {
        "readOnly": {
          "conditionGroup": [
            {
              "type": "isNotEmpty",
              "attribute": "id"
            }
          ]
        }
      },
      "sheet": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "format",
              "value": "Excel"
            }
          ]
        }
      },
      "isFileHeaderRow": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "format",
              "value": [
                "CSV",
                "Excel"
              ]
            }
          ]
        }
      },
      "excludedNodes": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "format",
              "value": [
                "JSON",
                "XML"
              ]
            }
          ]
        }
      },
      "keptStringNodes": {
        "visible": {
          "conditionGroup": [
            {
              "type": "in",
              "attribute": "format",
              "value": [
                "JSON",
                "XML"
              ]
            }
          ]
        }
      },
      "fileFieldDelimiter": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "format",
              "value": "CSV"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "format",
              "value": "CSV"
            }
          ]
        }
      },
      "fileTextQualifier": {
        "visible": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "format",
              "value": "CSV"
            }
          ]
        },
        "required": {
          "conditionGroup": [
            {
              "type": "equals",
              "attribute": "format",
              "value": "CSV"
            }
          ]
        }
      }
    }
  }
}
