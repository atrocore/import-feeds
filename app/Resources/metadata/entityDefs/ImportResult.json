{
  "fields": {
    "name": {
      "type": "varchar",
      "required": true,
      "trim": true,
      "readOnly": true
    },
    "importFeed": {
      "type": "link",
      "readOnly": true
    },
    "entityName": {
      "type": "varchar",
      "readOnly": true,
      "layoutDetailDisabled": true,
      "layoutDetailSmallDisabled": true,
      "layoutListDisabled": true,
      "layoutListSmallDisabled": true,
      "layoutFilterDisabled": true
    },
    "state": {
      "type": "enum",
      "required": true,
      "fontSize": 1,
      "options": [
        "Pending",
        "Running",
        "Done"
      ],
      "optionColors": {
        "Pending": "00AFFF",
        "Running": "FFE208",
        "Done": "1DFF08"
      },
      "default": "Pending",
      "readOnly": true
    },
    "start": {
      "notNull": false,
      "type": "datetime",
      "readOnly": true
    },
    "end": {
      "notNull": false,
      "type": "datetime",
      "readOnly": true
    },
    "createdCount": {
      "type": "int",
      "notStorable": true,
      "readOnly": true,
      "view": "import:views/import-result/fields/created-count"
    },
    "updatedCount": {
      "type": "int",
      "notStorable": true,
      "readOnly": true,
      "view": "import:views/import-result/fields/updated-count"
    },
    "errorsCount": {
      "type": "int",
      "notStorable": true,
      "readOnly": true,
      "view": "import:views/import-result/fields/errors-count"
    },
    "attachment": {
      "type": "file",
      "noAsset": true,
      "readOnly": true
    },
    "errorsAttachment": {
      "type": "file",
      "noAsset": true,
      "readOnly": true,
      "tooltip": true
    },
    "importResultLogs": {
      "type": "linkMultiple",
      "layoutDetailDisabled": true,
      "layoutMassUpdateDisabled": true,
      "noLoad": true,
      "importDisabled": true
    },
    "createdAt": {
      "type": "datetime",
      "readOnly": true
    },
    "modifiedAt": {
      "type": "datetime",
      "readOnly": true
    },
    "createdBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "modifiedBy": {
      "type": "link",
      "readOnly": true,
      "view": "views/fields/user"
    },
    "isRestored": {
      "type": "bool",
      "readOnly": true
    }
  },
  "links": {
    "importFeed": {
      "type": "belongsTo",
      "foreign": "importResults",
      "entity": "ImportFeed"
    },
    "attachment": {
      "type": "belongsTo",
      "entity": "Attachment",
      "skipOrmDefs": true
    },
    "errorsAttachment": {
      "type": "belongsTo",
      "entity": "Attachment",
      "skipOrmDefs": true
    },
    "importResultLogs": {
      "type": "hasMany",
      "foreign": "importResult",
      "entity": "ImportResultLog"
    },
    "createdBy": {
      "type": "belongsTo",
      "entity": "User"
    },
    "modifiedBy": {
      "type": "belongsTo",
      "entity": "User"
    }
  },
  "collection": {
    "sortBy": "createdAt",
    "asc": false
  },
  "indexes": {
    "name": {
      "columns": [
        "name",
        "deleted"
      ]
    }
  }
}
